# AI Backend: "github" or "openai"
AI_BACKEND=github

# Only needed if AI_BACKEND=openai
OPENAI_API_KEY=sk-your-key-here

# AI Model for hint generation
AI_MODEL=gpt-4o-mini

# JWT Configuration
JWT_SECRET=change-this-to-a-secure-random-string
JWT_EXPIRE_HOURS=24

# Database
DATABASE_URL=sqlite:///./deepml.db

# Docker Sandbox
SANDBOX_IMAGE=deepml-sandbox:latest
SANDBOX_TIMEOUT=30
SANDBOX_MEMORY=512m
